{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "todofiles",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_category"
        },
        {
          "type": "SYMBOL",
          "name": "task"
        }
      ]
    },
    "_category": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "category_definition"
        }
      ]
    },
    "task": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "todo_definition"
        },
        {
          "type": "SYMBOL",
          "name": "done_definition"
        },
        {
          "type": "SYMBOL",
          "name": "cancelled_definition"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "done_tag": {
      "type": "PATTERN",
      "value": "@done\\(\\d{2,4}.\\d{2}.\\d{2} \\d{2}:\\d{2}\\)\\n"
    },
    "cancelled_tag": {
      "type": "PATTERN",
      "value": "@cancelled\\(\\d{2,4}.\\d{2}.\\d{2} \\d{2}:\\d{2}\\)\\n"
    },
    "checkbox": {
      "type": "PATTERN",
      "value": "\\x{2610}"
    },
    "checkmark": {
      "type": "PATTERN",
      "value": "\\x{2714}"
    },
    "cancelled_mark": {
      "type": "PATTERN",
      "value": "\\x{2612}"
    },
    "category_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "\\t"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "category_expression"
        }
      ]
    },
    "todo_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "\\t"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "checkbox"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "done_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "\\t"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "checkmark"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        },
        {
          "type": "SYMBOL",
          "name": "done_tag"
        }
      ]
    },
    "cancelled_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "\\t"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "cancelled_mark"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        },
        {
          "type": "SYMBOL",
          "name": "cancelled_tag"
        }
      ]
    },
    "category_expression": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "REPEAT",
                "content": {
                  "type": "PATTERN",
                  "value": "\\t"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": ".*"
          },
          {
            "type": "PATTERN",
            "value": ":"
          },
          {
            "type": "PATTERN",
            "value": "\\n"
          }
        ]
      }
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}