{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "todofiles",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_category"
        }
      ]
    },
    "_category": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "category_definition"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_category"
              },
              {
                "type": "SYMBOL",
                "name": "data"
              }
            ]
          }
        }
      ]
    },
    "task": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "todo_definition"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "done_definition"
            },
            {
              "type": "SYMBOL",
              "name": "done_tag"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "cancelled_definition"
            },
            {
              "type": "SYMBOL",
              "name": "cancelled_tag"
            }
          ]
        }
      ]
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment_expression"
        }
      ]
    },
    "data": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "task"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "newline": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\n?"
      }
    },
    "category_definition": {
      "type": "SYMBOL",
      "name": "category_expression"
    },
    "todo_definition": {
      "type": "SYMBOL",
      "name": "todo_expression"
    },
    "done_definition": {
      "type": "SYMBOL",
      "name": "done_expression"
    },
    "cancelled_definition": {
      "type": "SYMBOL",
      "name": "cancelled_expression"
    },
    "comment_definition": {
      "type": "SYMBOL",
      "name": "comment_expression"
    },
    "done_tag": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@done.*"
      }
    },
    "cancelled_tag": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@cancelled.*"
      }
    },
    "category_expression": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": ".*"
          },
          {
            "type": "PATTERN",
            "value": ":"
          }
        ]
      }
    },
    "todo_expression": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\x{2610} "
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "done_expression": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\x{2714} "
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "cancelled_expression": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\x{2612} "
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "comment_expression": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "\\t"
            }
          },
          {
            "type": "PATTERN",
            "value": "# .*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "_category"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}